{
  "name": "cielpicoduchamphtml",
  "version": "1.0.0",
  "description": "HTML generation for the Stat page and Signature of Ciel\"Pico\"Duchamp on NRPG",
  "main": "compile.js",
  "scripts": {
    "test": "node compile.js",
    "start": "node compile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zee1234/CielPicoDuchampHTML.git"
  },
  "author": "Zee1234",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zee1234/CielPicoDuchampHTML/issues"
  },
  "homepage": "https://github.com/Zee1234/CielPicoDuchampHTML#readme",
  "dependencies": {
    "nib": "^1.1.2",
    "pretty-data": "^0.40.0",
    "pug": "^2.0.0-rc.2",
    "stylus": "^0.54.5",
    "yamljs": "^0.3.0"
  },
  "nexe": {
	"input": "./compile.js",
	"output": "nexe^$",
	"temp": "src",
	"browserify": {
		"requires": [],
		"excludes": [
      "./lib-cov/stylus",
      "canvas"
    ],
		"paths": []
	},
	"runtime": {
		"framework": "node",
		"version": "5.6.0",
    "js-flags": "--use_strict",
		"ignoreFlags": true
	}
}
}
